<% if (session_token = `op signin --account my --raw`.strip) != "" %># Generated by kamal envify
GITHUB_TOKEN=<%= `gh config get -h github.com oauth_token`.strip %>
KAMAL_REGISTRY_PASSWORD=<%= `op read "op://Personal/docker.com/password" -n --session #{session_token} ` %>
RAILS_MASTER_KEY=<%= `cat config/master.key` %>
CLUSTER_HEADACHE_TRACKER_DATABASE_PASSWORD=<%= ENV["CLUSTER_HEADACHE_TRACKER_DATABASE_PASSWORD"] %>
HONEYBADGER_API_KEY=<%= ENV["HONEYBADGER_API_KEY"] %>
<% else raise ArgumentError, "Session token missing" end %>
